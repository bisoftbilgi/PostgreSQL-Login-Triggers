# Extension description shown in pg_extension system catalog
comment = 'Password policy enforcement using check_password_hook'

# Version of the extension (corresponds to .sql file, e.g., password_check--1.0.sql)
default_version = '1.0'

# Whether the extension can be moved to another schema
relocatable = false

# The shared object file to load (without .so extension)
module_pathname = 'password_check'

